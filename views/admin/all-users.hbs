<section>
    <div class="container">
        <div class="col-md-4 mt-5">
            <table class="table table-dark">
                <thead>
                    <tr>
                        <th scope="col">Name</th>
                        <th scope="col">Email</th>
                        
                    </tr>
                </thead>
                <tbody>
                    {{#each users}}
                    <tr>
                        <td class="font-weight-bold">{{this.Name}}</td>
                        <td class="font-weight-bold">{{this.Email}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</section>